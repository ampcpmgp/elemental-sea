    "type-check": "tsc",
    "models:google": "bun run src/show-models/google.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@elemental-sea/tsconfig": "workspace:*",
    "@elemental-sea/vitest-config": "workspace:*",
    "@types/jsdom": "^21.1.6",
    "@vitest/coverage-v8": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.4.4",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.1.16",
    "@langchain/google-genai": "^0.0.11",
    "@langchain/openai": "^0.0.28",
    "emoji-regex": "^10.3.0",
    "langchain": "^0.1.34",
    "langsmith": "^0.1.14",
    "ts-pattern": "^5.1.1"
  },
  "overrides": {
    "@langchain/core": "0.1.58"
  }
