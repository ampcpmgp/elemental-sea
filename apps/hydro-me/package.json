{
  "name": "@elemental-sea/hydro-me",
  "version": "0.0.0",
  "type": "module",
  "main": "src/main.ts",
  "private": true,
  "scripts": {
    "sns-post": "bun run src/main.ts",
    "sns-post:dev": "bun run src/main.ts --watch",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "bunx @biomejs/biome lint .",
    "format": "bunx @biomejs/biome format . --write",
